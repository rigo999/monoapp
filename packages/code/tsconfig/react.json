{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Vite React",
    "extends": "./base.json",
    "compilerOptions": {
        "useDefineForClassFields": true,
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "module": "ESNext",
        "moduleResolution": "bundler",
        "jsx": "react-jsx",
        "noEmit": true,
        "allowImportingTsExtensions": true,
        "noUnusedParameters": true
    }
}
